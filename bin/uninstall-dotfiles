#!/bin/bash











### Un-Link .RC Files

# Ack
rm -s $HOME/.ackrc

# Sublime Text 3 Theme Default
rm -s $HOME/Library/Application\ Support/Sublime\ Text\ 3/Packages/Theme\ -\ Default

# ZProfile
rm $HOME/.zprofile

# Oh-My-Zsh
rm $HOME/.zshrc

# VIM
rm $HOME/.vimrc
rm $HOME/.gvimrc
rm $HOME/.vim
rm $HOME/.tern-config

# NODE
rm $HOME/.noderc

# Tig Pager
rm $HOME/.tigrc

# JSHint
rm $HOME/.jshintrc

# IRSSI
rm $HOME/.irssi/config

# EditorConfig
ln -s $DIR/.editorconfig/config $HOME/.editorconfig
